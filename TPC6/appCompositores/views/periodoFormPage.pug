extends layout 

block content
    .w3-card-4
        header.w3-container.w3-purple
            h2 Período Form
        form.w3-container(method="POST")
            fieldset
                legend Metadata
                label Id
                input.w3-input.w3-round(type="text" name="id")
                label Período
                input.w3-input.w3-round(type="text" name="periodo")
                label Compositores
                input.w3-input.w3-round(type="text" name="compositores")
            br
            button.w3-btn.w3-purple.w3-mb-2(type="submit") Register
        footer.w3-container.w3-purple
            h5Generated by EngWeb2023 in #{d} - [
                a(href='/') Return ]
                span 
      
                
            
   
